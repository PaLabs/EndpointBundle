<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="PaLabs\EndpointBundle\Loader\EndpointRouteLoader"
                 class="PaLabs\EndpointBundle\Loader\EndpointRouteLoader" public="false">
            <tag name="routing.loader"/>
        </service>

        <service id="PaLabs\EndpointBundle\Cache\EndpointRouteCacheWarmer"
                 class="PaLabs\EndpointBundle\Cache\EndpointRouteCacheWarmer" public="false">
            <tag name="kernel.cache_warmer"/>
        </service>

    </services>

</container>